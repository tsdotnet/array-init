{
  "name": "@tsdotnet/array-init",
  "version": "1.0.11",
  "description": "Commonly used array initializer that optimizes for length.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/arrayInit.d.ts",
  "main": "dist/arrayInit.js",
  "module": "dist-esm/arrayInit.js",
  "keywords": [
    "array",
    "array helper",
    "array utility",
    "array init",
    "array initialization"
  ],
  "scripts": {
    "build:dist": "rimraf dist/* && copyfiles -u 1 src/**/*.d.ts dist && tsc",
    "build:dist-esm": "rimraf dist-esm/* && tsc -p tsconfig.esm.json",
    "build": "pnpm i && run-p build:*",
    "bump": "npm run precommit && npm version patch",
    "docs": "rimraf docs/* && rimraf docs/.nojekyll && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && run-p lint test && run-p build:* && npm run validate && git status -s",
    "prepublishOnly": "npm run build && run-p validate test",
    "preversion": "run-p lint test",
    "postversion": "git push && git push --tags && npm run docs && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/array-init.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/array-init/issues"
  },
  "homepage": "https://github.com/tsdotnet/array-init#readme",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "eslint": "^7.24.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^4.2.4"
  }
}
